# fibonacci.err - Recursive Fibonacci demonstrating Turing completeness

function fib(n)
    if n <= 1
        return n
    end
    return fib(n - 1) + fib(n - 2)
end

main
    println("Fibonacci sequence:")
    println("fib(0) =", fib(0))
    println("fib(1) =", fib(1))
    println("fib(2) =", fib(2))
    println("fib(5) =", fib(5))
    println("fib(10) =", fib(10))

    gutter
        # Error zone - something may break
        println("Inside gutter: ", fib(
    end

    println("Program complete!")
end
